    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.kohlerbear.whowascnscalc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kohlerbear.whowascnscalc.Second"
            android:label="@string/title_activity_second" >
        </activity>
        <activity android:name="com.kohlerbear.whowascnscalc.SecondScreenActivity" >
        </activity>
        <activity android:name="com.kohlerbear.whowascnscalc.ThirdScreenActivity" >
            <intent-filter>
                <action android:name="com.kohlerbear.whowascnscalc.Second" />

                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.kohlerbear.whowascnscalc.IndividualView" />
            </intent-filter>
        </activity>
        <activity android:name="com.kohlerbear.whowascnscalc.IndividualViewActivity" >
            <intent-filter>
                <action android:name="com.kohlerbear.whowascnscalc.IndividualViewTwoActivity" />
            </intent-filter>
        </activity>
        <activity android:name="com.kohlerbear.whowascnscalc.IndividualViewTwoActivity" >
