        <activity android:name="com.kohlerbear.whowascnscalc.IndividualViewThreeActivity" >
        </activity>
        <activity
            android:name="com.kohlerbear.whowascnscalc.AdjustLiftPatternActivity"
            android:label="@string/title_activity_adjust_lift_pattern" >
        </activity>
    </application>

</manifest>